<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
  <connectionStrings>
    <add name="StockConnectionString" connectionString="data source=.;initial catalog=Stock;integrated security=True;"/>
  </connectionStrings>
  <appSettings>
    <add key="StockList" value="sz002236|2015-01-01" />
    <add key="BranchCount" value="10" />
    <!-- DailyIncreaseRole,XDaysIncreaseRole,DeltaIncreaseRole -->
    <add key="RoleList" value="DeltaIncreaseRole" />
    <add key="DaysForXDaysIncreaseRole" value="10" />
    <add key="LengthForDeltaIncreaseRole" value="60" />
    <add key="CoefficientForDeltaIncreaseRole" value="1.5" />
    <add key="MinOffsetForDeltaIncreaseRole" value="0.008" />
    <add key="PriceOffset" value="0.1" />
    <add key="HeapOffset" value="0.003" />
  </appSettings>
</configuration>
